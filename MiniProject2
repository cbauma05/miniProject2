void calc_diff(){

double latsum2, longsum2, altsum2;
double totalSum2;
double totalSum2sqrt;

latsum2 = (lat1 - lat2)*(lat1 - lat2);
longsum2 = (long1 - long2)*(long1 - long2);
altsum2 = (alt1 - alt2)*(alt1 - alt2);

totalSum2 = latsum2 + longsum2 + altsum2;

totalSum2sqrt = sqrt(totalSum2);

}
